package com.example.demo.dto;

import java.util.Date;

public class TreningDTO {
	private String naziv;
	private Date datum;
	private String[] nazivVezbi;
	private int[] serije;
	private int[] ponavljanja;
	
	public String getNaziv() {
		return naziv;
	}
	public void setNaziv(String naziv) {
		this.naziv = naziv;
	}
	public Date getDatum() {
		return datum;
	}
	public void setDatum(Date datum) {
		this.datum = datum;
	}
	public String[] getNazivVezbi() {
		return nazivVezbi;
	}
	public void setNazivVezbi(String[] nazivVezbi) {
		this.nazivVezbi = nazivVezbi;
	}
	public int[] getSerije() {
		return serije;
	}
	public void setSerije(int[] serije) {
		this.serije = serije;
	}
	public int[] getPonavljanja() {
		return ponavljanja;
	}
	public void setPonavljanja(int[] ponavljanja) {
		this.ponavljanja = ponavljanja;
	}
}